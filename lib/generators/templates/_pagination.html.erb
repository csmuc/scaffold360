<%%= will_paginate(collection, :params=>{:format=>nil} ) %>
<hr>
<%%= will_paginate(collection, :remote=>{:replace=>'.<%= plural_name %>_remote_target'}, :params=>{:format=>:htmlf}, :renderer=>Scaffold360::WillPaginate::RemoteLinkRenderer) %>

